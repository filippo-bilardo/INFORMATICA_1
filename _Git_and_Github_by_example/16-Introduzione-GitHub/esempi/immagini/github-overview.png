<?xml version="1.0" encoding="UTF-8"?>
<svg width="900" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="900" height="600" fill="#f8f9fa"/>
  
  <!-- Title -->
  <text x="450" y="30" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2c3e50">GitHub Overview - Panoramica della Piattaforma</text>
  
  <!-- GitHub logo area -->
  <rect x="50" y="60" width="150" height="150" fill="#24292f" stroke="#f6f8fa" stroke-width="2" rx="10"/>
  <text x="125" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="white">GitHub</text>
  <text x="125" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7d8590">Code hosting</text>
  <text x="125" y="150" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7d8590">& collaboration</text>
  <text x="125" y="170" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7d8590">platform</text>
  
  <!-- Features grid -->
  <rect x="230" y="70" width="150" height="80" fill="#d1ecf1" stroke="#bee5eb" stroke-width="2" rx="5"/>
  <text x="305" y="90" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#0c5460">📁 Repositories</text>
  <text x="305" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#0c5460">Hosting codice</text>
  <text x="305" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#0c5460">Gestione progetti</text>
  
  <rect x="400" y="70" width="150" height="80" fill="#d4edda" stroke="#c3e6cb" stroke-width="2" rx="5"/>
  <text x="475" y="90" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#155724">🔀 Pull Requests</text>
  <text x="475" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#155724">Code Review</text>
  <text x="475" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#155724">Collaborazione</text>
  
  <rect x="570" y="70" width="150" height="80" fill="#f8d7da" stroke="#f5c6cb" stroke-width="2" rx="5"/>
  <text x="645" y="90" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#721c24">🐛 Issues</text>
  <text x="645" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#721c24">Bug tracking</text>
  <text x="645" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#721c24">Feature requests</text>
  
  <rect x="740" y="70" width="150" height="80" fill="#fff3cd" stroke="#ffeeba" stroke-width="2" rx="5"/>
  <text x="815" y="90" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#856404">⚙️ Actions</text>
  <text x="815" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#856404">CI/CD</text>
  <text x="815" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#856404">Automazione</text>
  
  <!-- Second row -->
  <rect x="230" y="170" width="150" height="80" fill="#e2e3e5" stroke="#d6d8db" stroke-width="2" rx="5"/>
  <text x="305" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#383d41">🌐 Pages</text>
  <text x="305" y="210" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#383d41">Hosting statico</text>
  <text x="305" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#383d41">Siti web gratis</text>
  
  <rect x="400" y="170" width="150" height="80" fill="#d1ecf1" stroke="#bee5eb" stroke-width="2" rx="5"/>
  <text x="475" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#0c5460">👥 Teams</text>
  <text x="475" y="210" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#0c5460">Gestione utenti</text>
  <text x="475" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#0c5460">Permessi</text>
  
  <rect x="570" y="170" width="150" height="80" fill="#d4edda" stroke="#c3e6cb" stroke-width="2" rx="5"/>
  <text x="645" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#155724">📊 Projects</text>
  <text x="645" y="210" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#155724">Kanban boards</text>
  <text x="645" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#155724">Project management</text>
  
  <rect x="740" y="170" width="150" height="80" fill="#f8d7da" stroke="#f5c6cb" stroke-width="2" rx="5"/>
  <text x="815" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#721c24">🔒 Security</text>
  <text x="815" y="210" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#721c24">Vulnerability alerts</text>
  <text x="815" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#721c24">Code scanning</text>
  
  <!-- Workflow diagram -->
  <rect x="50" y="280" width="800" height="280" fill="#2c3e50" stroke="#34495e" stroke-width="2" rx="5"/>
  <text x="60" y="305" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ecf0f1">Workflow GitHub Tipico</text>
  
  <!-- Workflow steps -->
  <circle cx="120" cy="350" r="25" fill="#3498db"/>
  <text x="120" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">1</text>
  <text x="120" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Fork/Clone</text>
  <text x="120" y="405" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Repository</text>
  
  <!-- Arrow 1 -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#ecf0f1"/>
    </marker>
  </defs>
  <line x1="150" y1="350" x2="200" y2="350" stroke="#ecf0f1" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <circle cx="230" cy="350" r="25" fill="#27ae60"/>
  <text x="230" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">2</text>
  <text x="230" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Crea Branch</text>
  <text x="230" y="405" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Sviluppa</text>
  
  <!-- Arrow 2 -->
  <line x1="260" y1="350" x2="310" y2="350" stroke="#ecf0f1" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <circle cx="340" cy="350" r="25" fill="#e74c3c"/>
  <text x="340" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">3</text>
  <text x="340" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Commit</text>
  <text x="340" y="405" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Push</text>
  
  <!-- Arrow 3 -->
  <line x1="370" y1="350" x2="420" y2="350" stroke="#ecf0f1" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <circle cx="450" cy="350" r="25" fill="#f39c12"/>
  <text x="450" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">4</text>
  <text x="450" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Pull Request</text>
  <text x="450" y="405" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Code Review</text>
  
  <!-- Arrow 4 -->
  <line x1="480" y1="350" x2="530" y2="350" stroke="#ecf0f1" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <circle cx="560" cy="350" r="25" fill="#9b59b6"/>
  <text x="560" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">5</text>
  <text x="560" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Merge</text>
  <text x="560" y="405" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#ecf0f1">Deploy</text>
  
  <!-- Benefits section -->
  <text x="60" y="450" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#ecf0f1">Vantaggi GitHub:</text>
  <text x="60" y="475" font-family="Arial, sans-serif" font-size="12" fill="#3498db">🌟 Backup cloud sicuro del codice</text>
  <text x="60" y="495" font-family="Arial, sans-serif" font-size="12" fill="#27ae60">🤝 Collaborazione semplificata</text>
  <text x="60" y="515" font-family="Arial, sans-serif" font-size="12" fill="#e74c3c">📝 Documentazione integrata</text>
  <text x="60" y="535" font-family="Arial, sans-serif" font-size="12" fill="#f39c12">🚀 CI/CD automation</text>
  
  <text x="450" y="475" font-family="Arial, sans-serif" font-size="12" fill="#9b59b6">👁️ Visibilità progetti open source</text>
  <text x="450" y="495" font-family="Arial, sans-serif" font-size="12" fill="#1abc9c">🎯 Issue tracking integrato</text>
  <text x="450" y="515" font-family="Arial, sans-serif" font-size="12" fill="#e67e22">📊 Analytics e insights</text>
  <text x="450" y="535" font-family="Arial, sans-serif" font-size="12" fill="#34495e">🔐 Controllo accessi granulare</text>
</svg>
